    </Compile>
    <Compile Update="Vistas\Cliente\Agregar.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Vistas\Cliente\Editar.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Vistas\Cliente\Listado.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Vistas\Empleado\Editar.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Vistas\Empleado\Agregar.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Vistas\Empleado\Listado.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Vistas\Producto\Agregar.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Vistas\Producto\Editar.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Vistas\Producto\Listado.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Vistas\Producto\ProductData.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Vistas\Ventas\Agregar.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Vistas\Ventas\Editar.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Vistas\Ventas\Listado.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Vistas\Ventas\VentaDataForm.cs">
      <SubType>Form</SubType>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>